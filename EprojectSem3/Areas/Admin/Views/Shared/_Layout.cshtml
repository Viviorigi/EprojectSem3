    <script src="~/assets/admin/assets/plugins/slimscroll/jquery.slimscroll.min.js" type="9cdf354c52c4c31d84896533-text/javascript"></script>

    <!-- Map JS -->
    <script src="~/assets/admin/assets/plugins/slimscroll/jquery.slimscroll.min.js" type="9cdf354c52c4c31d84896533-text/javascript"></script>
